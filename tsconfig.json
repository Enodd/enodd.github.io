{
  "extends": "astro/tsconfigs/base",
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "@data/*": ["src/data/*"],
      "@components/*": ["src/components/*"],
      "@hooks/*": ["src/hooks/*"],
      "@models/*": ["src/models/*"],
      "@layouts/*": ["src/layouts/*"],
      "@styles/*": ["src/styles"],
      "@pages/*": ["src/pages/*"],
      "@providers/*": ["src/providers/*"],
      "@views/*": ["src/views/*"],
    },
  }
}